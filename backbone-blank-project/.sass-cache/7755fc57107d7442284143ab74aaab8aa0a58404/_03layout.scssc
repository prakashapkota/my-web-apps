                }
            }
            .ctm-live-stram{
                width: 100%;
                .ctm-title-logo{

                }
                .video-info{
                    padding-top: 105px;
                    #player{
                        text-align: center;
                        .videoWrapper {
                            width: 90%;
                            margin: 0 auto;
                            position: relative;
                            padding-bottom: 56.25%; /* 16:9 */
                            padding-top: 25px;
                            height: 0;
                        }
                        .videoWrapper iframe {
                            position: absolute;
                            top: 0;
                            left: 0;
                            width: 100%;
                            height: 100%;
                        }
                    }
                    .info {
                        width: 100%;
                        .googleMap{
                            display: none;
                        }
                        .info-text{
                            float: none;
                            text-align: center;
                            width: auto;
                            padding: 15px;
                        }
                    }
                }
            }
        }


        #ctm-mobile-share{
            position: fixed;
            bottom: 0;
            left: 0;
            height: 40px;
            width: 100%;
            background: #000;
            z-index: 99;
            text-align: center;
            ul{
                display: inline-block;
                padding-top: 5px;
                margin: 0 auto;
                li{
                    float: left;
                    margin-right: 15px;
                    a{
                        display: inline-block;
                        width: 30px;
                        height: 30px;
                        &.fb{
                            background: url(../img/mobile/btn-share.png) 0px 0px no-repeat;
                        }
                        &.tw{
                            background: url(../img/mobile/btn-share.png) -40px 0px no-repeat;
                        }
                        &.ggl{
                            background: url(../img/mobile/btn-share.png) -80px 0px no-repeat;
                        }

                    }
                }
            }
        }
    }
}

/* Tablets */

@media only screen 
and (min-device-width : 768px) 
and (max-device-width : 1024px) {
    
    #ctm-wrapper {
        
        background: url(../img/sky-tablet.jpg) no-repeat center center fixed;
        -webkit-background-size: cover;
        -moz-background-size: cover;
        -o-background-size: cover;
        background-size: cover;
    
        #nav{
            .nav-wrapper{ width: 100%; }
        }
        #page {
            
            .ctm-teaser{
                width: 100%;
                
                .ctm-moon-visuel{
                    
                    #video {
                        //visibility: hidden!important;
                        position: relative;
                        z-index: 1;
                    }
                    
                    .moon-container{
                        
                        .ipad-mask {
                            width: 400px;
                            height: 400px;
                            position: absolute;
                            z-index: 2;
                            //visibility: hidden!important;
                        }
                    }
                }
            }
            .ctm-register {
                width: 100%;
            }
        }
    }
}

/* Tablets (landscape) ----------- */
@media only screen 
and (min-device-width : 768px) 
and (max-device-width : 1024px) 
and (orientation : landscape) {
    
    #ctm-wrapper {
        #page {
            .ctm-teaser{
                .ctm-moon-visuel{
                    
                    margin-left: -150px;
                    
                    .moon-container{
                        
                        width: 297px;
                        height: 297px;
                        border-radius: 297px;
                        
                        
                        .ipad-mask {
                            background: url(../img/ipad-mask-horizontal.png) left top no-repeat;
                        }
                        
                        #video {
                            width: 300px;
                            height: 300px;
                        }
                    }
                    
                    .halo {
                        width: 406px;
                        top: 83px;
                        left: -53px;
                    }
                }
                
                .ctm-bottom-info {
                    top: 480px;
                }
            }
        }
    }
}

/* Tablets (portrait) ----------- */
@media only screen 
and (min-device-width : 768px) 
and (max-device-width : 1024px) 
and (orientation : portrait) {

    #ctm-wrapper {
        #page {
            .ctm-teaser{
                .ctm-moon-visuel{
                    .moon-container{
                        .ipad-mask {
                            background: url(../img/ipad-mask-vertical.png) left top no-repeat;
                        }
                    }
                }
            }
            
            .ctm-title-logo, .ctm-register-form, .register-bottom-visuel {
                margin-top: 70px;
            }
            
            .ctm-moon-visuel {
                margin-top: 80px;
            }
            .ctm-bottom-info {
                margin-top: 110px;
            }
        }
    }
}
