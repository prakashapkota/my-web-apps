:@template"à#game {
    width: 100%;
    margin: 0 auto;
    position: relative;
    padding-top: 113px;
    
    .logo-color { display: none }
    
    .canvas-container {
        
        position: relative;
        width: 504px;
        height: 490px;
        margin: 0 auto;
        
        #canvas {
            cursor: url(../img/cursor.cur), auto;
        }
        
        .picker {
            display: none;
            width: 37px;
            height: 39px;
            background: url(../img/picker.png) left top no-repeat;
            position: absolute;
            top: 170px;
            left: 50%;
            margin-left: -19px;
        }
    }
    
    
    
    .moon {
        position: absolute;
        top: 43px;
        left: 50%;
        margin-left: 220px;
        width: 137px;
        height: 137px;
        overflow: visible;
        padding: 10px;
        
        .blur {
            position: absolute;
            z-index: 0;
            left: -31px;
            top: -31px;
            width: 200px;
            height: 200px;
            background: url(../img/halo-small.png) left top no-repeat;
            
            /*
            filter: blur(5px);
            -webkit-filter: blur(25px);
            -moz-filter: blur(25px);
            -o-filter: blur(25px);
            -ms-filter: blur(25px);
            */
        }
        
        .mask {
            position: absolute;
            z-index: 1;
            left: 0;
            top: 0;
            width: 137px;
            height: 137px;
            border-radius: 69px;
            background-color: white;
            behavior: url(js/lib/PIE.htc);
            overflow: visible;
        }
        
        img {
            position: absolute;
            z-index: 2;
            left: 0;
            top: 0;
        }
    }
    
    .steps {
        
        width: 317px;
        height: 150px;
        position: absolute;
        top: 230px;
        left: 50%;
        margin-left: -158px;
        font-family: 'pfsquaresanspro-regular', Arial, Helvetica, sans-serif;
        color: white;
        font-size: 1em;
        line-height: 1.6em;
        text-align: center;
        
        .button {
            text-transform: uppercase;
            display: block;
            padding: 5px 0;
            margin: 15px auto 0;
            border: 1px white solid;
            color: white;
            text-decoration: none;
            width: 125px;
            
            &:hover {
                background: #0b528e;
                color: #fff;
                
                -webkit-transition: all .3s ease-in;
                -moz-transition: all .3s ease-in;
                -ms-transition: all .3s ease-in;
                -o-transition: all .3s ease-in;
                transition: all .3s ease-in;
            }
        }
        
        .step-01 {
            width: 317px;
            position: absolute;
            top: 20px;
            
            .color {
                
                height: 30px;
                overflow: hidden;
                position: relative;
                
                strong {
                    font-size: 1.5em;
                    display: block;
                    height: 30px;
                    position: relative;
                    text-transform: uppercase;
                    font-size: 2em;
                    letter-spacing: -0.03em;
                    font-weight: bold;
                    
                    &.last {
                        display: block;
                        height: 30px;
                        position: absolute;
                        top: -30px;
                        left: 0;
                        width: 100%;
                        text-align: center;
                    }
                }
            }
            
            .logo { vertical-align: text-bottom; }
            .error { 
                visibility: hidden;
                position: relative;
                top: 20px;
                font-size: .8em;
            }
            
            p {
                -webkit-touch-callout: none;
                -webkit-user-select: none;
                -khtml-user-select: none;
                -moz-user-select: none;
                -ms-user-select: none;
                user-select: none;
            }
        }
        
        .step-02 {
            width: 317px;
            display: none;
            position: absolute;
            top: 0;
            
            &.twitter { top: 40px; }
            
            .name { color: #5090ae }
            
            .fb-button {
                display: block;
                padding: 5px 10px 0 47px;
                margin-bottom: 10px;
                height: 31px;
                background: #457e9b url(../img/logo-fb-small.gif) left top no-repeat;
                text-shadow: 0 1px 1px #141516;
                text-decoration: none;
                text-align: left;
                display: inline-block;
                position: relative;
                
                &:after {
                    content: ' ';
                    display: block;
                    height: 36px;
                    width: 1px;
                    position: absolute;
                    left: 37px;
                    top: 0;
                    background: url(../img/btn-separator.gif) left top no-repeat;
                }
            }
            
            a {
                color: white;
                text-decoration: underline;
                
                &.fb-share, &.twitter-share {
                    display: block;
                    width: 36px;
                    height: 36px;
                    position: absolute;
                    bottom: -55px;
                }
                
                &.fb-share {
                    background: url(../img/logo-fb-small.gif) left top no-repeat;
                    left: 115px;
                }
                
                &.twitter-share {
                    background: url(../img/logo-twitter-small.gif) left top no-repeat;
                    left: 165px;
                }
            }
        }
    }
}

// Smartphones
@media only screen 
and (min-device-width : 320px) 
and (max-device-width : 568px) {
    
    #game {
        padding: 0;
        
        .logo-color { 
            display: block;
            width: 276px;
            margin: 20px auto 10px;
        }
        
        .canvas-container {
            width: 100%;
            height: auto;
            z-index: 4;
        }
        
        .moon {
            width: 120px;
            height: 120px;
            left: 50%;
            top: 104px;
            margin-left: -60px;
            padding: 0;
            
            img {
                width: 120px;
                height: 120px;
            }
            
            .mask {
                width: 118px;
                height: 118px;
                left: 1px;
                top: 1px;
            }
            
            .blur {
                width: 160px;
                height: 160px;
                background-size: 100% auto;
                left: -20px;
                top: -20px;
            }
        }
        
        .steps {
            
            font-size: 1em;
            top: 270px;
            line-height: 1.4em;
            
            .step-01 {
                strong { font-size: 1.5em; }
            }
            
            .break {
                display: none;
            }
        }
    }
}

/* Tablets (landscape) ----------- */
@media only screen 
and (min-device-width : 768px) 
and (max-device-width : 1024px) 
and (orientation : landscape) {
    
    #game {
        padding-top: 50px;
        
        .steps {
            top: 167px;
        }
        
        .moon {
            left: 53%;
        }
    }
